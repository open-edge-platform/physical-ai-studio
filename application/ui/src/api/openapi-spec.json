{
    "openapi": "3.1.0",
    "info": {
        "title": "Geti Action",
        "description": "Geti Action is a framework to train robots.It allows the user to create datasets, models and the run inference ",
        "version": "0.1.0"
    },
    "paths": {
        "/api/projects": {
            "get": {
                "summary": "List Projects",
                "description": "Fetch all projects",
                "operationId": "list_projects_api_projects_get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": { "$ref": "#/components/schemas/Project" },
                                    "type": "array",
                                    "title": "Response List Projects Api Projects Get"
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "summary": "Create Project",
                "description": "Create a new project",
                "operationId": "create_project_api_projects_put",
                "requestBody": {
                    "content": { "application/json": { "schema": { "$ref": "#/components/schemas/Project" } } },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": { "application/json": { "schema": { "$ref": "#/components/schemas/Project" } } }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": { "schema": { "$ref": "#/components/schemas/HTTPValidationError" } }
                        }
                    }
                }
            }
        },
        "/api/projects/{id}": {
            "get": {
                "summary": "Get Project",
                "description": "Get project by id",
                "operationId": "get_project_api_projects__id__get",
                "parameters": [
                    { "name": "id", "in": "path", "required": true, "schema": { "type": "string", "title": "Id" } }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": { "schema": { "$ref": "#/components/schemas/ProjectConfig" } }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": { "schema": { "$ref": "#/components/schemas/HTTPValidationError" } }
                        }
                    }
                }
            }
        },
        "/api/projects/{project_id}/datasets/{repo}/{id}": {
            "get": {
                "summary": "Get Dataset Of Project",
                "description": "Get dataset of project by id",
                "operationId": "get_dataset_of_project_api_projects__project_id__datasets__repo___id__get",
                "parameters": [
                    {
                        "name": "project_id",
                        "in": "path",
                        "required": true,
                        "schema": { "type": "string", "title": "Project Id" }
                    },
                    { "name": "repo", "in": "path", "required": true, "schema": { "type": "string", "title": "Repo" } },
                    { "name": "id", "in": "path", "required": true, "schema": { "type": "string", "title": "Id" } }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": { "application/json": { "schema": { "$ref": "#/components/schemas/Dataset" } } }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": { "schema": { "$ref": "#/components/schemas/HTTPValidationError" } }
                        }
                    }
                }
            }
        },
        "/api/hardware/cameras": {
            "get": {
                "summary": "Get Cameras",
                "description": "Get all cameras",
                "operationId": "get_cameras_api_hardware_cameras_get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": { "$ref": "#/components/schemas/Camera" },
                                    "type": "array",
                                    "title": "Response Get Cameras Api Hardware Cameras Get"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/hardware/robots": {
            "get": {
                "summary": "Get Robots",
                "description": "Get all connected Robots",
                "operationId": "get_robots_api_hardware_robots_get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": { "$ref": "#/components/schemas/RobotPortInfo" },
                                    "type": "array",
                                    "title": "Response Get Robots Api Hardware Robots Get"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/hardware/calibrations": {
            "get": {
                "summary": "Get Lerobot Calibrations",
                "description": "Get calibrations known to huggingface leRobot",
                "operationId": "get_lerobot_calibrations_api_hardware_calibrations_get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": { "$ref": "#/components/schemas/CalibrationConfig" },
                                    "type": "array",
                                    "title": "Response Get Lerobot Calibrations Api Hardware Calibrations Get"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/hardware/identify": {
            "put": {
                "summary": "Identify Robot",
                "description": "Visually identify the robot by moving given joint on robot",
                "operationId": "identify_robot_api_hardware_identify_put",
                "parameters": [
                    {
                        "name": "joint",
                        "in": "query",
                        "required": false,
                        "schema": { "anyOf": [{ "type": "string" }, { "type": "null" }], "title": "Joint" }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": { "application/json": { "schema": { "$ref": "#/components/schemas/RobotPortInfo" } } }
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": { "application/json": { "schema": {} } }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": { "schema": { "$ref": "#/components/schemas/HTTPValidationError" } }
                        }
                    }
                }
            }
        },
        "/api/hardware/camera_feed": {
            "get": {
                "summary": "Get Camera Feed",
                "description": "Get a streaming response from the camera",
                "operationId": "get_camera_feed_api_hardware_camera_feed_get",
                "parameters": [
                    { "name": "id", "in": "query", "required": true, "schema": { "type": "string", "title": "Id" } },
                    {
                        "name": "type",
                        "in": "query",
                        "required": true,
                        "schema": { "enum": ["RealSense", "OpenCV"], "type": "string", "title": "Type" }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": { "application/json": { "schema": {} } }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": { "schema": { "$ref": "#/components/schemas/HTTPValidationError" } }
                        }
                    }
                }
            }
        },
        "/api/cameras/offer/camera": {
            "post": {
                "summary": "Offer Camera",
                "description": "Create a WebRTC offer",
                "operationId": "offer_camera_api_cameras_offer_camera_post",
                "parameters": [
                    {
                        "name": "camera",
                        "in": "query",
                        "required": true,
                        "schema": { "type": "string", "title": "Camera" }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": { "application/json": { "schema": { "$ref": "#/components/schemas/Offer" } } }
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": { "application/json": { "schema": { "$ref": "#/components/schemas/Answer" } } }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": { "schema": { "$ref": "#/components/schemas/HTTPValidationError" } }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Answer": {
                "properties": {
                    "sdp": { "type": "string", "title": "Sdp" },
                    "type": { "type": "string", "title": "Type" }
                },
                "type": "object",
                "required": ["sdp", "type"],
                "title": "Answer"
            },
            "CalibrationConfig": {
                "properties": {
                    "id": { "type": "string", "title": "Id" },
                    "path": { "type": "string", "title": "Path" },
                    "robot_type": { "type": "string", "enum": ["teleoperator", "robot"], "title": "Robot Type" }
                },
                "type": "object",
                "required": ["id", "path", "robot_type"],
                "title": "CalibrationConfig"
            },
            "Camera": {
                "properties": {
                    "name": { "type": "string", "title": "Name", "description": "Camera name" },
                    "id": {
                        "type": "string",
                        "title": "Id",
                        "description": "Either serial id for  RealSense or port for OpenCV"
                    },
                    "type": { "type": "string", "enum": ["RealSense", "OpenCV"], "title": "Type" },
                    "default_stream_profile": { "$ref": "#/components/schemas/CameraProfile" }
                },
                "type": "object",
                "required": ["name", "id", "type", "default_stream_profile"],
                "title": "Camera"
            },
            "CameraConfig": {
                "properties": {
                    "id": {
                        "type": "string",
                        "maxLength": 50,
                        "minLength": 1,
                        "title": "Id",
                        "description": "Camera port or realsense id"
                    },
                    "name": {
                        "type": "string",
                        "maxLength": 50,
                        "minLength": 1,
                        "title": "Name",
                        "description": "Camera name"
                    },
                    "type": { "type": "string", "enum": ["RealSense", "OpenCV"], "title": "Type" },
                    "width": { "type": "integer", "title": "Width", "description": "Frame width", "default": 640 },
                    "height": { "type": "integer", "title": "Height", "description": "Frame height", "default": 480 },
                    "fps": { "type": "integer", "title": "Fps", "description": "Camera fps", "default": 30 },
                    "use_depth": {
                        "type": "boolean",
                        "title": "Use Depth",
                        "description": "Use Depth from RealSense",
                        "default": false
                    }
                },
                "type": "object",
                "required": ["id", "name", "type"],
                "title": "CameraConfig"
            },
            "CameraProfile": {
                "properties": {
                    "width": { "type": "integer", "title": "Width" },
                    "height": { "type": "integer", "title": "Height" },
                    "fps": { "type": "integer", "title": "Fps" }
                },
                "type": "object",
                "required": ["width", "height", "fps"],
                "title": "CameraProfile"
            },
            "Dataset": {
                "properties": {
                    "repo_id": { "type": "string", "title": "Repo Id" },
                    "episodes": {
                        "items": { "$ref": "#/components/schemas/Episode" },
                        "type": "array",
                        "title": "Episodes"
                    },
                    "total_frames": { "type": "integer", "title": "Total Frames" },
                    "features": { "items": { "type": "string" }, "type": "array", "title": "Features" },
                    "tasks": { "items": { "type": "string" }, "type": "array", "title": "Tasks" },
                    "fps": { "type": "integer", "title": "Fps" }
                },
                "type": "object",
                "required": ["repo_id", "episodes", "total_frames", "features", "tasks", "fps"],
                "title": "Dataset"
            },
            "Episode": {
                "properties": {
                    "episode_index": { "type": "integer", "title": "Episode Index" },
                    "length": { "type": "integer", "title": "Length" },
                    "fps": { "type": "integer", "title": "Fps" },
                    "tasks": { "items": { "type": "string" }, "type": "array", "title": "Tasks" },
                    "actions": {
                        "items": { "items": { "type": "number" }, "type": "array" },
                        "type": "array",
                        "title": "Actions"
                    },
                    "modification_timestamp": { "type": "integer", "title": "Modification Timestamp" }
                },
                "type": "object",
                "required": ["episode_index", "length", "fps", "tasks", "actions", "modification_timestamp"],
                "title": "Episode"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": { "$ref": "#/components/schemas/ValidationError" },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "Offer": {
                "properties": {
                    "sdp": { "type": "string", "title": "Sdp" },
                    "type": { "type": "string", "title": "Type" },
                    "webrtc_id": { "type": "string", "title": "Webrtc Id" }
                },
                "type": "object",
                "required": ["sdp", "type", "webrtc_id"],
                "title": "Offer"
            },
            "Project": {
                "properties": {
                    "id": { "type": "string", "format": "uuid", "title": "Id" },
                    "name": { "type": "string", "title": "Name", "default": "Default Name" }
                },
                "type": "object",
                "title": "Project",
                "example": { "id": "7b073838-99d3-42ff-9018-4e901eb047fc", "name": "SO101 Teleoperation" }
            },
            "ProjectConfig": {
                "properties": {
                    "id": { "type": "string", "title": "Id", "description": "UUID of the project" },
                    "fps": {
                        "type": "integer",
                        "title": "Fps",
                        "description": "Recording FPS for datasets",
                        "default": 30
                    },
                    "name": {
                        "type": "string",
                        "maxLength": 50,
                        "minLength": 1,
                        "title": "Name",
                        "description": "Project name"
                    },
                    "datasets": {
                        "items": { "type": "string" },
                        "type": "array",
                        "title": "Datasets",
                        "description": "Datasets available for this project",
                        "default": []
                    },
                    "cameras": {
                        "items": { "$ref": "#/components/schemas/CameraConfig" },
                        "type": "array",
                        "title": "Cameras"
                    },
                    "robots": {
                        "items": { "$ref": "#/components/schemas/RobotConfig" },
                        "type": "array",
                        "title": "Robots"
                    }
                },
                "type": "object",
                "required": ["id", "name", "cameras", "robots"],
                "title": "ProjectConfig"
            },
            "RobotConfig": {
                "properties": {
                    "id": { "type": "string", "title": "Id", "description": "Robot calibration id" },
                    "type": { "type": "string", "enum": ["follower", "leader"], "title": "Type" },
                    "serial_id": { "type": "string", "title": "Serial Id", "description": "Serial port id" }
                },
                "type": "object",
                "required": ["id", "type", "serial_id"],
                "title": "RobotConfig"
            },
            "RobotPortInfo": {
                "properties": {
                    "port": { "type": "string", "title": "Port" },
                    "serial_id": { "type": "string", "title": "Serial Id" },
                    "device_name": { "type": "string", "title": "Device Name" }
                },
                "type": "object",
                "required": ["port", "serial_id", "device_name"],
                "title": "RobotPortInfo"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": { "type": "string", "title": "Message" },
                    "type": { "type": "string", "title": "Error Type" }
                },
                "type": "object",
                "required": ["loc", "msg", "type"],
                "title": "ValidationError"
            }
        }
    }
}
